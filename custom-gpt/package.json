{
  "name": "custom-gpt",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler worker deploy ./build/client",
    "dev": "remix vite:dev --port 5173",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler worker dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "db:deploy": "wrangler d1 migrations apply custom-gpt-db"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@remix-run/server-runtime": "^2.16.8",
    "aws4": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "isbot": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.513.0",
    "mongodb": "^6.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@remix-run/dev": "^2.16.8",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.84.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
